import LayoutTwo from "@layouts/LayoutTwo.astro";
import PlanAll from "@components/PlanAll.astro";
import images from "@data/imagesA";
---

<LayoutTwo title="Villas">
  <VillasCard />
  <PlanAll
    para="Contamos con villas con un concepto moderno desde 2 personas y hasta 4 personas con accesos a bordo de carretera, instalaciones seguras y con privacidad. Un clima de montaña agradable con excelentes miradores a los volcanes y cerca de cafeterías artesanales."
    imagen={images.planVilla}
    imagen2={images.planVilla2}
    imagen3={images.planVilla3}
    imagen4={images.planVilla4}
    imagen5={images.planVilla5}
  />
</LayoutTwo>
